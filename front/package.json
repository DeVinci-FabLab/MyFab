{
  "name": "myfabultimate_front",
  "description": "The FrontEnd for MyFab",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "buildStart": "npm run build && npm run start",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:server": "start-server-and-test dev http://localhost:3000/dvfl/auth cy:run",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "linter": "npx prettier -w ."
  },
  "dependencies": {
    "@headlessui/react": "^2.1.1",
    "@heroicons/react": "^2.1.4",
    "@next/bundle-analyzer": "^14.2.4",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "cookies-next": "^4.2.1",
    "moment": "^2.30.1",
    "next": "14.1.4",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-moment": "^1.1.3",
    "react-stl-viewer": "^2.5.0",
    "react-toastify": "^10.0.5",
    "sha256": "^0.2.0",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.14.2",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4"
  }
}
